Cell[TextData[{
 "Yep. Now, here is another small quiz: convert ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"10", "cos", 
         RowBox[{"(", 
           RowBox[{"0.6", "\[Pi]", 
