f94050cdce9a"],

Cell[BoxData[
 RowBox[{
  RowBox[{"PatternMultiplication", "[", 
   RowBox[{"elem_", ",", "af_"}], "]"}], ":=", 
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"elem1", "=", 
       RowBox[{"elem", "/.", 
        RowBox[{"\[Theta]", "->", 
         RowBox[{
          FractionBox["\[Pi]", "2"], "-", "\[Theta]"}]}]}]}], ",", 
