 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"-", "\[Beta]"}], TraditionalForm], "errors" -> {}, "input" -> 
     "-\\beta", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "45290120-938e-4cb6-b109-7a6e72299f37"],
