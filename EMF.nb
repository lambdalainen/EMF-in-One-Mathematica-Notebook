              "]"}], "/.", 
             RowBox[{"\[Omega]", "->", 
              RowBox[{"0.6", "\[Pi]"}]}]}], ",", 
            RowBox[{
             RowBox[{"Re", "[", 
              RowBox[{"f2", " ", 
               SuperscriptBox["\[ExponentialE]", 
                RowBox[{"\[ImaginaryJ]", " ", "\[Omega]", " ", "t"}]]}], 
              "]"}], "/.", 
             RowBox[{"\[Omega]", "->", 
              RowBox[{"0.6", "\[Pi]"}]}]}], ",", 
            RowBox[{
             RowBox[{"Re", "[", 
              RowBox[{
               RowBox[{"(", 
                RowBox[{"f1", "+", "f2"}], ")"}], 
               SuperscriptBox["\[ExponentialE]", 
                RowBox[{"\[ImaginaryJ]", " ", "\[Omega]", " ", "t"}]]}], 
              "]"}], "/.", 
             RowBox[{"\[Omega]", "->", 
              RowBox[{"0.6", "\[Pi]"}]}]}]}], "}"}], ",", 
          RowBox[{"{", 
           RowBox[{"x", ",", 
            RowBox[{"-", "2"}], ",", "5"}], "}"}], ",", 
          RowBox[{"AxesLabel", "->", "Automatic"}], ",", 
