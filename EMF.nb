Cell[TextData[{
 "Once we write ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|"boxes" -> FormBox[
       RowBox[{"\[CapitalGamma]", "\[LongEqual]", 
         FractionBox[
          RowBox[{
