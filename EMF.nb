             RowBox[{"SphericalPlot3D", "[", 
              RowBox[{
               RowBox[{"Abs", "[", "prod", "]"}], ",", 
               RowBox[{"{", 
                RowBox[{"\[Theta]", ",", "0", ",", "\[Pi]"}], "}"}], ",", 
               RowBox[{"{", 
                RowBox[{"\[Phi]", ",", "0", ",", 
                 RowBox[{"2", "\[Pi]"}]}], "}"}], ",", 
               RowBox[{"PlotRange", "->", 
