Cell[BoxData[
 RowBox[{
  RowBox[{"WaveAlongZ", "[", "incident_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "incidentAbs", ",", "plotRange", ",", "reflected", ",", "transmitted", 
      ",", "\[CapitalGamma]", ",", "\[Tau]"}], "}"}], ",", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"incidentAbs", "=", 
       RowBox[{"Abs", "[", 
        RowBox[{"incident", "/.", 
         RowBox[{"z", "->", "0"}]}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"plotRange", "=", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{
          RowBox[{
           RowBox[{"-", "incidentAbs"}], "*", "2"}], "+", "2"}], ",", 
         RowBox[{
          RowBox[{"incidentAbs", "*", "2"}], "-", "2"}]}], "}"}]}], ";", " ", 
      
      RowBox[{"(*", " ", 
       RowBox[{
       "With", " ", "the", " ", "permittivity", " ", "values", " ", "we", " ",
         "know", " ", "the", " ", "total", " ", "wave", " ", 
        RowBox[{"won", "'"}], "t", " ", "exceed", " ", "this", " ", "range"}],
        " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"reflected", "=", 
