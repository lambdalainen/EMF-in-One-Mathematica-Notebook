              SubscriptBox["k", "2"], 
              SubscriptBox["k", "1"]], "=", 
             FractionBox[
              SqrtBox[
               SubscriptBox["\[Epsilon]", "r2"]], 
              SqrtBox[
               SubscriptBox["\[Epsilon]", "r1"]]]}]}], " ", "*)"}], 
          "\[IndentingNewLine]", 
          RowBox[{"Animate", "[", 
           RowBox[{
            RowBox[{"Legended", "[", 
             RowBox[{
              RowBox[{"Plot", "[", 
               RowBox[{
                RowBox[{"{", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"Piecewise", "[", 
                   RowBox[{"{", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"Re", "[", 
                    RowBox[{"incident", "*", 
                    SuperscriptBox["\[ExponentialE]", 
                    RowBox[{"\[ImaginaryJ]", " ", "\[Omega]", " ", "t"}]]}], 
                    "]"}], " ", "/.", 
                    RowBox[{"\[Omega]", "->", 
                    RowBox[{"0.3", "\[Pi]"}]}]}], ",", 
                    RowBox[{"z", "<", "0"}]}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"NaN", ",", 
                    RowBox[{"z", ">", "0"}]}], "}"}]}], "}"}], "]"}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{"Piecewise", "[", 
                   RowBox[{"{", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"Re", "[", 
                    RowBox[{"\[CapitalGamma]", "*", "reflected", "*", 
                    SuperscriptBox["\[ExponentialE]", 
                    RowBox[{"\[ImaginaryJ]", " ", "\[Omega]", " ", "t"}]]}], 
                    "]"}], " ", "/.", 
                    RowBox[{"\[Omega]", "->", 
                    RowBox[{"0.3", "\[Pi]"}]}]}], ",", 
                    RowBox[{"z", "<", "0"}]}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"NaN", ",", 
                    RowBox[{"z", ">", "0"}]}], "}"}]}], "}"}], "]"}], ",", 
                  "\[IndentingNewLine]", 
                  RowBox[{"Piecewise", "[", 
                   RowBox[{"{", 
                    RowBox[{
                    RowBox[{"{", 
                    RowBox[{"NaN", ",", 
                    RowBox[{"z", "<", "0"}]}], "}"}], ",", 
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"Re", "[", 
                    RowBox[{"\[Tau]", "*", "transmitted", "*", 
                    SuperscriptBox["\[ExponentialE]", 
                    RowBox[{"\[ImaginaryJ]", " ", "\[Omega]", " ", "t"}]]}], 
                    "]"}], "/.", 
                    RowBox[{"\[Omega]", "->", 
                    RowBox[{"0.3", "\[Pi]"}]}]}], ",", 
                    RowBox[{"z", ">", "0"}]}], "}"}]}], "}"}], "]"}], ",", 
