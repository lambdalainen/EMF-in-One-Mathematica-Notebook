Cell[BoxData[
 RowBox[{
  RowBox[{"WaveReflection", "[", "incident_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "reflected", "}"}], ",", 
    RowBox[{"(", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"reflected", "=", 
       RowBox[{"incident", "/.", 
        RowBox[{"z", "->", 
         RowBox[{"-", "z"}]}]}]}], ";", 
      RowBox[{"Manipulate", "[", 
       RowBox[{
        RowBox[{"Animate", "[", 
         RowBox[{
          RowBox[{"Legended", "[", 
           RowBox[{
            RowBox[{"Plot", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{
                 RowBox[{"Re", "[", 
                  RowBox[{"incident", " ", 
