       RowBox[{"af", "/.", 
        RowBox[{"\[Theta]", "->", 
         RowBox[{
          FractionBox["\[Pi]", "2"], "-", "\[Theta]"}]}]}]}], ",", 
      RowBox[{"prod", "=", 
       RowBox[{"elem", "*", "af"}]}], ",", 
      RowBox[{"prod1", "=", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"elem", "*", "af"}], ")"}], "/.", 
        RowBox[{"\[Theta]", "->", 
         RowBox[{
          FractionBox["\[Pi]", "2"], "-", "\[Theta]"}]}]}]}]}], "}"}], ",", 
    RowBox[{"Manipulate", "[", 
     RowBox[{
      RowBox[{"Legended", "[", 
       RowBox[{
        RowBox[{"Grid", "[", 
         RowBox[{"{", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"PolarPlot", "[", 
              RowBox[{
               RowBox[{"Abs", "[", "elem1", "]"}], ",", 
