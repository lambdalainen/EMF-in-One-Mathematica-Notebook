            RowBox[{"\"\<\[Beta]\>\"", ",", "\[Beta]"}], "}"}], "}"}]}], 
         "]"}]}], "]"}], ",", 
      RowBox[{"{", 
       RowBox[{"t", ",", "0", ",", "5"}], "}"}]}], "]"}], ",", 
    RowBox[{"{", 
     RowBox[{"\[Beta]", ",", "1", ",", "3"}], "}"}]}], "]"}]}]], "Input",
