Cell[TextData[{
 "Neat! Now I can see that the sign in front of ",
 Cell[BoxData[
  FormBox[
   TemplateBox[<|
    "boxes" -> FormBox["\[Beta]", TraditionalForm], "errors" -> {}, "input" -> 
     "\\beta", "state" -> "Boxes"|>,
    "TeXAssistantTemplate"], TraditionalForm]],ExpressionUUID->
  "b7ae38cb-324d-484e-84eb-4fe6bafa58af"],
